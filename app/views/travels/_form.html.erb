<%= form_with model: travel do |f| %>
  <div class="form-group">
    <%= f.label :場所や地名 %>
    <%= f.text_field :title, class: 'form-control travel_title' %>
  </div>
  <div class="form-group">
  <%= f.label :画像 %>
  <%= f.file_field :image, accept: "image/*" %>
  </div>
  <div class="form-group">
    <%= f.label :感想 %>
    <%= f.text_area :body, class: 'form-control travel_body' %>
  </div>

  <div class="form-group">
    <%= f.submit "投稿", class: 'btn btn-outline-success rounded-pill' %>
  </div>

<% end %>